=form_tag(search_path, method: "get", id: "user-search-form", data:{form: "search-form"} ) do
  .form-group=text_field_tag :search, params[:search], placeholder: "Search Your Collection", class: "form-control"
  =submit_tag "Search", name: nil, class: "btn btn-default", id: "search_button",data:{button: "search-button"}